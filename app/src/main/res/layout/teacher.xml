<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#E4E4E6">

    <TextView
        android:id="@+id/textView"
        android:layout_width="264dp"
        android:layout_height="53dp"
        android:text="Ronald Bartels"
        android:textColor="#000000"
        android:textSize="40sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.572"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.053" />

    <TextView
        android:id="@+id/sessionLabel1"
        android:layout_width="263dp"
        android:layout_height="31dp"
        android:text="Tutorial (11/11/22 - 9:30AM)"
        android:textColor="#000000"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/teacherButton1"
        app:layout_constraintHorizontal_bias="0.638"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:layout_constraintVertical_bias="0.175" />

    <TextView
        android:id="@+id/sessionLabel2"
        android:layout_width="263dp"
        android:layout_height="31dp"
        android:text="Workshop (9/11/22 - 12:30PM)"
        android:textColor="#000000"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/teacherButton1"
        app:layout_constraintHorizontal_bias="0.636"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sessionLabel1"
        app:layout_constraintVertical_bias="0.054" />

    <TextView
        android:id="@+id/sessionLabel"
        android:layout_width="263dp"
        android:layout_height="31dp"
        android:text="Workshop (8/10/22 - 2:30PM)"
        android:textColor="#000000"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/teacherButton1"
        app:layout_constraintHorizontal_bias="0.636"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sessionLabel1"
        app:layout_constraintVertical_bias="0.166" />

    <TextView
        android:id="@+id/sessionLabel3"
        android:layout_width="263dp"
        android:layout_height="31dp"
        android:text="Lecture (8/10/22 - 12:30PM)"
        android:textColor="#000000"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/teacherButton1"
        app:layout_constraintHorizontal_bias="0.638"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sessionLabel1"
        app:layout_constraintVertical_bias="0.273" />

    <Button
        android:id="@+id/teacherButton1"
        android:layout_width="96dp"
        android:layout_height="39dp"
        android:layout_marginEnd="16dp"
        android:backgroundTint="#E16D6D"
        android:text="View"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:layout_constraintVertical_bias="0.177" />

    <Button
        android:id="@+id/teacherButton2"
        android:layout_width="97dp"
        android:layout_height="36dp"
        android:layout_marginEnd="16dp"
        android:backgroundTint="#E16D6D"
        android:text="View"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/teacherButton1"
        app:layout_constraintVertical_bias="0.036" />

    <Button
        android:id="@+id/teacherButton"
        android:layout_width="97dp"
        android:layout_height="36dp"
        android:layout_marginEnd="16dp"
        android:backgroundTint="#E16D6D"
        android:text="View"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/teacherButton1"
        app:layout_constraintVertical_bias="0.146" />

    <Button
        android:id="@+id/teacherButton3"
        android:layout_width="97dp"
        android:layout_height="36dp"
        android:layout_marginEnd="16dp"
        android:backgroundTint="#E16D6D"
        android:text="View"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/teacherButton1"
        app:layout_constraintVertical_bias="0.254" />

    <Button
        android:id="@+id/viewStudentBtn"
        android:layout_width="132dp"
        android:layout_height="55dp"
        android:layout_marginEnd="16dp"
        android:backgroundTint="#E16D6D"
        android:text="Search"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView9"
        app:layout_constraintVertical_bias="0.195" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="264dp"
        android:layout_height="38dp"
        android:layout_marginBottom="44dp"
        android:text="Check Student Attendance"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/editTextTextPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sessionLabel"
        app:layout_constraintVertical_bias="1.0" />

    <EditText
        android:id="@+id/editTextTextPersonName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="140dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Student Number"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/viewStudentBtn"
        app:layout_constraintHorizontal_bias="0.754"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/editTextTextPersonName2"
        android:layout_width="137dp"
        android:layout_height="39dp"
        android:layout_marginStart="32dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Date/Time"
        android:textColor="#000000"
        app:layout_constraintBottom_toTopOf="@+id/sessionLabel1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:layout_constraintVertical_bias="0.588" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="59dp"
        android:layout_height="28dp"
        android:layout_marginStart="116dp"
        android:text="Filter"
        android:textSize="22sp"
        app:layout_constraintBottom_toTopOf="@+id/sessionLabel1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:layout_constraintVertical_bias="0.506" />

    <Button
        android:id="@+id/teacherLogoutBtn"
        android:layout_width="120dp"
        android:layout_height="57dp"
        android:backgroundTint="#E16D6D"
        android:text="Logout"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName"
        app:layout_constraintVertical_bias="0.807" />

</androidx.constraintlayout.widget.ConstraintLayout>